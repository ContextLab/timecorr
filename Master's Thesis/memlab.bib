@article{AbboEtal97,
	Abstract = {Cortical neurons receive synaptic inputs from thousands of afferents
	that fire action potentials at rates ranging from less than 1 hertz
	to more than 200 hertz. Both the number of afferents and their large
	dynamic range can mask changes in the spatial and temporal pattern
	of synaptic activity, limiting the ability of a cortical neuron to
	respond to its inputs. Modeling work based on experimental measurements
	indicates that short-term depression of intracortical synapses provides
	a dynamic gain-control mechanism that allows equal percentage rate
	changes on rapidly and slowly firing afferents to produce equal postsynaptic
	responses. Unlike inhibitory and adaptive mechanisms that reduce
	responsiveness to all inputs, synaptic depression is input-specific,
	leading to a dramatic increase in the sensitivity of a neuron to
	subtle changes in the firing patterns of its afferents.},
	Author = {L. F. Abbott and J. A. Varela and K. Sen and S. B. Nelson},
	Date-Added = {2014-04-04 16:37:02 +0000},
	Date-Modified = {2014-04-04 16:37:29 +0000},
	Institution = {Volen Center, Brandeis University, Waltham, MA 02254, USA.},
	Journal = {Science},
	Keywords = {Action Potentials; Animals; Electric Stimulation; Models, Neurological; Neuronal Plasticity; Neurons; Neurons, Afferent; Rats; Synapses; Synaptic Transmission; Visual Cortex},
	Number = {5297},
	Owner = {per},
