	Keywords = {Adult, Cognition, Consciousness, Extramural, Humans, Memory, N.I.H., Recognition (Psychology), Research Support, Temporal Lobe, 16723542},
	Month = {May},
	Number = {21},
	Owner = {per},
	Pages = {5835--5839},
	Pii = {26/21/5835},
	Pmid = {16723542},
	Timestamp = {2006.06.12},
	Title = {{T}he medial temporal lobe distinguishes old from new independently of consciousness.},
