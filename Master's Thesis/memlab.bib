	Journal = JNSCI,
	Number = {18},
	Pages = {7284-92},
	Title = {Dentate gyrus and ca1 ensemble activity during spatial reference frame shifts in the presence and absence of visual input.},
	Volume = {21},
	Year = {2001},
	Bdsk-File-1 = {}}

