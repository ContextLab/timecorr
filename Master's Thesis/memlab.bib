	Pages = {163--175},
	Pii = {S1053-8119(98)90328-7},
	Pmid = {9597658},
	Timestamp = {2006.04.28},
	Title = {{F}unctional-anatomic study of episodic retrieval. {II}. {S}elective averaging of event-related f{MRI} trials to test the retrieval success hypothesis.},
	Url = {http://dx.doi.org/10.1006/nimg.1998.0328},
