	Journal = {J Exp Psychol Learn Mem Cogn},
	Journal-Full = {Journal of experimental psychology. Learning, memory, and cognition},
	Mesh = {Consciousness; Cues; Female; Form Perception; Humans; Male; Memory; Pattern Recognition, Visual; Time Factors},
	Month = {Jan},
	Number = {1},
	Pages = {127-37},
