	Month = {Nov},
	Number = {9},
	Pages = {1150--1162},
	Pii = {S0893-6080(05)00198-X},
	Pmid = {16198538},
	Timestamp = {2006.10.20},
	Title = {{P}lace from time: {R}econstructing position from a distributed representation of temporal context.},
