@article{JensTesc00,
	Annote = {***(REDO THIS)*** Previously a physiological network model for short-term
	memory (STM) has been suggested in which brain oscillations in the
	5-11 Hz band (theta/alpha band) are involved in active maintenance
	and recall of STM representations (Lisman and Idiart, Science 267:1512;
	Jensen and Lisman, JNS 18:10668). This model can account for the
	reaction time distributions of the classical STM paradigm: the Sternberg
	task. In this task subjects are required to retain a list of 1,3,
	5 or 7 digits during a 3 second retention period. We investigated
	the role of theta activity in a visual Sternberg task using a whole-scalp
	magnetoencephalographic (MEG) array. Responses from 10 normal subjects
	revealed ongoing theta activity as a peak in the power spectrum recorded
	by frontal midline sensors. During the retention period, the power
	of the theta activity increased parametrically with the number of
	items retained in STM. A time-frequency analysis revealed that the
	theta activity remained strong during the full retention period.
	These observations are in accord with the predictions of the oscillatory
	model of short-term memory.},
	Author = {Jensen, O. and Tesche, C. D.},
	Date-Added = {2014-04-04 16:37:02 +0000},
	Date-Modified = {2014-04-04 16:37:21 +0000},
	Journal = {Society for Neuroscience Abstracts},
	Title = {Ongoing theta activity (6-8 Hz) over the frontal midline increases with memory load in a short-term memory task: a parametric MEG study},
	Year = {2000}}

