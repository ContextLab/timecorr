@article{CaplEtal01,
	Annote = {$\blacksquare$ The authors present a new method for analyzing episodes
	of oscillatry activity and apply this method to the analysis of iEEG
	data during maze learning. Using a larger dataset than used by Kahana
	et al (1999), and more careful controls for epileptic artifacts,
	Caplan et al demonstrate that the maze-length effect (increased theta
	activity in longer mazes) was specific to the theta and low alpha
	bands. Hypothesizing that the maze length effect reflected increased
	encoding/retrieval difficulty in longer mazes, they examined the
	relation between theta episodes and time-per-junction, the time subjects
	spend deliberating at a junction before making a turn. Contrary to
	expectations, this measure of encoding/retrieval difficulty did not
	correlate with theta activity. Instead, gamma activity increased
	during periods of increased deliberation at maze junctions. These
	findings suggest that theta's role in cognition is a general one,
	with theta activity increasing during periods of hightened attention,
	but not as a specific response to the difficulty of individual choices.
	This interpretation is also consistent with Raghavachari's findings
	of gated theta oscillations during the Sternberg working memory task.},
	Author = {Caplan, J. B. and Madsen, J. R. and Raghavachari, S. and Kahana, M. J.},
	Date-Added = {2014-04-04 16:37:02 +0000},
	Date-Modified = {2014-04-04 16:37:10 +0000},
	Journal = {Journal of Neurophysiology},
	Pages = {368--380},
	Title = {Distinct patterns of brain oscillations underlie two basic parameters of human maze learning},
