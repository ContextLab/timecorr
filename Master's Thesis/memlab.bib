	Journal = {Journal of Anatomy},
	Keywords = {Amnesia; Diencephalon; Hippocampus; Humans; Magnetic Resonance Imaging; Memory; Models, Psychological; Neuropsychological Tests; Positron-Emission Tomography; Semantics; Temporal Lobe},
	Number = {1},
