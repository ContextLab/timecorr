	Mesh = {Adaptation, Physiological; Adult; Analysis of Variance; Brain Mapping; Computer Simulation; Extremities; Face; Female; Humans; Image Processing, Computer-Assisted; Magnetic Resonance Imaging; Male; Models, Neurological; Oxygen; Pattern Recognition, Visual; Photic Stimulation; Temporal Lobe; Time Factors; Visual Pathways; Young Adult},
	Month = {Jun},
	Number = {6},
