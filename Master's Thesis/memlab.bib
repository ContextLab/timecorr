	Journal = {Nature Reviews Neuroscience},
	Keywords = {Animals, Brain Mapping, Cognition, Hippocampus, Humans, Nerve Net, Neural Pathways, Non-U.S. Gov't, P.H.S., Research Support, U.S. Gov't, 16858394},
