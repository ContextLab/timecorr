	Journal = {J Neurosci},
	Keywords = {Animals, Cerebral Cortex, Electrophysiology, Female, Forecasting, Head, Inbred Strains, Movement, Neural Pathways, Non-P.H.S., Rats, Research Support, Thalamic Nuclei, Thalamus, Time Factors, U.S. Gov't, 7666208},
	Month = {Sep},
	Number = {9},
	Pages = {6260--6270},
	Pmid = {7666208},
	Timestamp = {2006.10.20},
	Title = {{A}nticipatory head direction signals in anterior thalamus: evidence for a thalamocortical circuit that integrates angular head motion to compute head direction.},
	Volume = {15},
	Year = {1995}}

