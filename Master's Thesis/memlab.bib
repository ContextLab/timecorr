@article{FinnRobe11,
	Annote = {Review evidence that high theta power in older adults is associated with MCI and transition to AD.

Review evidence that in young adults high theta power is actually associated with better performance on memory and attention tasks.

Some of their past work has shown that among healthy older adults decreasing theta power predicted decreasing cognitive performance 

Want to investigate this apparent ambiguity in a large sample of healthy olders 

EEG was recorded for 4min of eyes-closed rest. The resulting EEG was correlated with a variety of behavioral measures that were administered prior to the EEG recording (i.e., no EEG was collected during task performance).

Fz theta power was positivity correlated with several memory tests, ravens, and category fluency. It was negativity correlated with RT variability on the Sustained Attention to Response test. IT was uncorrelated with age, NART, MMSE, digit span forward and backward, other SART measures, anxiety scores, depression scores, and peak alpha frequency. Pz theta showed positive correlations only with a prose recall memory test (all other correlations with Pz theta were NS).

Suggest that the findings of increased theta power being associated with INCREASED risk of AD/MCI are due to general slowing of EEG rythms such that increased theta power comes from a shift down in the peak alpha frequency. By contrast, the current study did not find any effect of peak alpha frequency suggesting that the positive correlations between theta power and cognition are not due to such slowing.},
	Author = {Finnigan, Simon and Robertson, Ian H},
	Date-Added = {2014-04-04 16:37:02 +0000},
	Date-Modified = {2014-04-04 16:37:25 +0000},
	Journal = {Psychophysiology},
	Month = jan,
	Pages = {1083--1087},
	Title = {Resting EEG theta power correlates with cognitive performance in healthy older adults},
	Volume = {48},
	Year = {2011}}

