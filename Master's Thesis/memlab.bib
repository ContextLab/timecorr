	Institution = {Department of Psychology, University of Melbourne, Victoria, 3010, Australia. philipls@unimelb.edu.au},
	Journal = {Trends in Neurosciences},
	Keywords = {Action Potentials; Animals; Decision Making; Eye Movements; Humans; Information Theory; Models, Neurological; Models, Psychological; Neural Networks (Computer); Reaction Time},
	Number = {3},
