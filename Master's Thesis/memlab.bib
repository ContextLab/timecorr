@article{MarkCerm98,
	Abstract = {Current theories propose that amnesia is caused by an inability to
	encode the temporal properties of recent events and/or to associate
	information across time. The present investigation tested this postulation
	by manipulating the recency effect which is theorized to be caused
	by the encoding of temporal information. The continual-distractor
	paradigm was used to vary the temporal properties of recently presented
	lists. Amnesics' recall responded normally to the temporal manipulations
	in lists ranging from 18-54 s. In contrast, overall recall was impaired
	compared to normals in all conditions and across all positions, including
	the final position. These findings dissociate memory for temporal
	information from overall levels of recall. They suggest that the
	amnesic patient's memory deficit is not caused by an inability to
	encode temporally associated information.},
	Author = {A. R. Marks and L. S. Cermak},
	Date-Added = {2014-04-04 16:37:02 +0000},
	Date-Modified = {2014-04-04 16:37:16 +0000},
	Institution = {Memory Disorders Research Center, Boston University School of Medicine, USA. amarks@helix.nih.gov},
	Journal = {Neuropsychologia},
	Keywords = {Aged; Amnes; Analysis of Variance; Case-Control Studies; Humans; Memory Disorders; Memory, Short-Term; Mental Recall; Middle Aged; Models, Psychological; Time Factors; Time Perception; Verbal Learning; ia},
	Number = {9},
	Owner = {per},
